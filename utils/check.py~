def load(filename):
    instream = open(filename, 'r') 
    content = instream.readlines() 
    instream.close()
    return content

def split(L):
    list=L.split(',')
    i = 0
    while i < len(list):
        dict[list[i]]=list[i+1]
        i+=2
    return dict
        
if __name__=='__main__':
    print split(load("../data/data.csv"))
